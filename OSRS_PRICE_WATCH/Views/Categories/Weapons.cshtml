@model _Items[]


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>WeaponCategories</title>
</head>
<body>
    <br /><br /><br />
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Item Name</th>
                <th scope="col">Price</th>
                <th scope="col">High Alch</th>
                <th scope="col">Low Alch</th>
                <th scope="col">Wiki Link</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var weapons in Model)
            {
                <tr>
                    <td>@weapons.wiki_name</td>
                    <td>@weapons.cost</td>
                    <td>@weapons.highalch</td>
                    <td>@weapons.lowalch</td>
                    <td><a class="btn btn-outline-primary" href="@weapons.wiki_url">Wiki</a></td>
                    <td><a class="btn btn-primary">View More</a></td>
                </tr>
            }
        </tbody>
    </table>

</body>
</html>