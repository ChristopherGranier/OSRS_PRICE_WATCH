@model _Items[]
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>WeaponCategories</title>
</head>
<body>
    @foreach (var potions in Model)
    {
        <p>@potions.name</p>
        <p>@potions.cost</p>
    }
</body>
</html>
